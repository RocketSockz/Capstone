{
  "properties": {
    "document": {
      "properties": {
        "databases": {
          "items": {
            "if": {
              "properties": {
                "database_name": {
                  "not": {
                    "enumIgnoreCase": ["master", "tempdb", "msdb"]
                  }
                }
              }
            },
            "then": {
              "properties": {
                "guestAccess": {
                  "items": {
                    "if": {
                      "properties": {
                        "user_name": {
                          "constIgnoreCase": "guest"
                        }
                      }
                    },
                    "then": {
                      "properties": {
                        "hasdbaccess": {
                          "const": 0
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}