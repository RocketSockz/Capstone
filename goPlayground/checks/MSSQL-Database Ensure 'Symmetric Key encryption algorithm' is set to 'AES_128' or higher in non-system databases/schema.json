{
  "properties": {
    "document": {
      "properties": {
        "databases": {
          "items": {
            "if": {
              "properties": {
                "database_name": {
                  "not": {
                    "enumIgnoreCase": ["master", "msdb", "model", "tempdb"]
                  }
                }
              }
            },
            "then": {
              "properties": {
                "encryptionSettings": {
                  "items": {
                    "if": {
                      "properties": {
                        "key_type": {
                          "constIgnoreCase": "symmetric"
                        }
                      }
                    },
                    "then": {
                      "properties": {
                        "algorithm_desc": {
                          "enumIgnoreCase": ["AES_128", "AES_192", "AES_256"]
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}